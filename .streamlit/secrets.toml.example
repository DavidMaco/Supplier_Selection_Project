# AEGIS Streamlit Cloud Secrets
# Copy these key-value pairs into:
#   Streamlit Cloud → App Settings → Secrets
#
# [database]
# DB_HOST = "your-mysql-host.example.com"
# DB_PORT = "3306"
# DB_USER = "aegis_user"
# DB_PASSWORD = "your-password"
# DB_NAME = "aegis_procurement"
#
# [auth]
# AEGIS_DASHBOARD_USER = "admin"
# AEGIS_DASHBOARD_PASS_HASH = "sha256-hash-of-your-password"
